\\n    if(props.datesFrom && d.getTime() < props.datesFrom.getTime()){\\n      d = props.datesFrom;\\n    }\\n    else if (props.datesTo && d.getTime() < props.datesFrom.getTime()){\\n      d = props.datesTo;\\n    }\\n\\n    this.defaultDayIndex = d.getDate() -1;\\n    this.defaultMonthIndex = d.getMonth();\\n    this.defaultYear = d.getYear() + 1900;\\n    \\n   console.log(d);\\n    \\n    \\n    this.state = {\\n      value : d,\\n      dateValid: true,\\n      error: false,\\n      day: d.getDate(),\\n      month: DATE_LOCALE.MONTHSEN[d.getMonth()],\\n      year: parseInt(d.getFullYear()),\\n    };\\n    console.log(this.state);\\n    \\n  }  \\n  \\n  componentDidMount(){\\n    this.saveSelectionToState();\\n  }\\n    \\n  saveSelectionToState(){\\n\\n    \\n    const a = this.state.month + \\\" \\\" + this.state.day + \\\" \\\" + this.state.year;\\n    console.log(a);\\n    \\n    const sDate = new Date(a);\\n    \\n    console.log(sDate);\\n    \\n    if(this.isValidDate(sDate)){\\n      this.setState({\\n        value: sDate,\\n        dateValid: true,\\n        error: false,\\n      });\\n    }\\n    else {\\n      this.setState({\\n        value: sDate,\\n        dateValid: false,\\n      });\\n    }\\n  }\\n  \\n  dayChange(d){\\n    this.setState({\\n      day: parseInt(d),\\n    });\\n    \\n    this.saveSelectionToState();\\n  }\\n  \\n  monthChange(m){\\n    this.setState({\\n      month: DATE_LOCALE.MONTHSEN[DATE_LOCALE.MONTHS.indexOf(m)],\\n    });\\n    \\n    this.saveSelectionToState();\\n  }\\n  \\n  yearChange(y){\\n    this.setState({\\n      year: parseInt(y),\\n    });\\n    \\n    this.saveSelectionToState()\\n  }\\n  \\n  isValidDate(d){\\n    if (Object.prototype.toString.call(d) === \\\"[object Date]\\\") {\\n      if (!isNaN(d.getTime())) { \\n        return true;\\n      } \\n    } \\n    return false;\\n  }\\n  \\n  \\n   renderDaySelector(){\\n    const dayArr = [];\\n    for(let i = 1; i < 32; i++){\\n      dayArr.push(i + \\\"\\\"